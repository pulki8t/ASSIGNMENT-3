{
  console.log(i);
}


console.log("Question 10");
let X = prompt("Enter 1/2 number to print all nos not divisible by 3 in between");
let Y = prompt("Enter 2/2 number to print all nos not divisible by 3 in between");
for(let i = X; i<=Y; i++)
{
  if(i % 3 != 0)
  {
    console.log(i);
  }
}


console.log("Question 11");
let XX = prompt("Enter a number to print its double");
console.log("Double of " + XX + " = " + XX*2);


console.log("Question 12");
let day = prompt("Enter a day to find out whether its a weekend");
var Day = (day.slice(0,3)).toLowerCase();
if(Day == 'sat' || Day == 'sun')
{
  console.log("It is a weekend");
}
else
{
  console.log("It is a working day");
}


console.log("Question 13");
let fn = prompt("Enter number of terms in Fibonacci Series");
var f1 = 0;
var f2 = 1;
console.log(0);
for(let i = 0; i<fn; i++)
{
  console.log(f2);
  var F2 = f2;
  f2 = f1 + f2;
  f1 = F2;
}


console.log("Question 14");
let AB = prompt("Enter a number to print its square and cube");
console.log("Square of the " + AB + " = " + AB*AB);
console.log("Cube of the " + AB + " = " + AB*AB*AB);


console.log("Question 15");
document.getElementById("img").style.width = "0px";
function display(x)
{
  document.getElementById("img").style.width = "50px";
}
function nodisplay(x)
{
  document.getElementById("img").style.width = "0px";
}


console.log("Question 16");
let xyz = prompt("Enter 1/3 number to print largest among them");
let yyy = prompt("Enter 2/3 number to print largest among them");
let zz = prompt("Enter 3/3 number to print largest among them");
if (xyz>=yyy && xyz>=zz)
{
  console.log("Larger number is " + xyz );
}
else if(yyy>=xyz && yyy>=zz)
{
  console.log("Larger number is " + yyy );
}
else
{
  console.log("Larger number is " + zz );
}


console.log("Question 17");
let O = parseInt(prompt("Enter a number to print sum of 1 to it"));
console.log("The sum of 1 to " + O + " = " + O*(O+1)/2);


console.log("Question 18");
function changeColor()
{
  document.body.style.backgroundColor = "rgb(247, 158, 198)";
}
  function undoChange()
{
  document.body.style.backgroundColor = "rgb(255, 255, 255)";
}


console.log("Question 19");
let RS = prompt("Enter something to find whether it's an integer");
if(isNaN(RS))
{
  console.log("Not an Integer");
}
else
{
  console.log("It is an Integer");
}


console.log("Question 20");
let R = prompt("Enter a number to find whether it's +ve, -ve or 0");
if(R<0)
{
  console.log("It is a negative number");
}
else if(R>0)
{
  console.log("It is a Positive number");
}
else
{
  console.log(0);
}
