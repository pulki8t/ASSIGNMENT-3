console.log("Question 1");
var sum = 0;
for(let i = 1;i<=10;i++)
{
  sum += i;
}
console.log("Sum of first 10 natural nos. = "+sum);


console.log("Question 2");
var a = 15;
var b = 26;
var sum = 0;
for(let i = 15; i<=26; i++)
{
  sum += i;
}
console.log("Sum of all numbers between " + a + " and " + b + " = " + sum + " (including 15 & 26)");


console.log("Question 3");
for(let i=1; i<=10; i++)
{
  console.log("5 X " + i + " = " + 5*i);
}


console.log("Question 4");
let aa = prompt("Enter a number to print its table");
for(let i=1; i<=10; i++)
{
  console.log(aa + " X " + i + " = " + aa*i);
}


console.log("Question 5");
let x = prompt("Enter a number to calculate sum of its digits");
var xx = x;
var sum_5 = 0;
while(x != 0)
{
  sum_5 += x % 10;
  x = (x - (x % 10)) / 10;
}
console.log("Sum of all digits in " + xx + " = " + sum_5);


console.log("Question 6");
let y = prompt("Enter a number to reverse");
var yy = y;
var result = 0;
var m = 10 ** (y.length - 1);
while(y != 0)
{
  result += m * (y % 10);
  y = (y - (y % 10)) / 10;
  m /= 10;
}
console.log("Reverse of " + yy + " = " + result);


console.log("Question 7");
for(let i = 0; i<=10; i += 2)
{
  console.log(i);
}


console.log("Question 8");
let A = prompt("Enter 1/2 number to print all odd nos in between");
let B = prompt("Enter 2/2 number to print all odd nos in between");
for(let i = A; i<=B; i++)
{
  if(i % 2 != 0)
  {
    console.log(i);
  }
}


